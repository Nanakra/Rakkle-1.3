<Defs>

	<RecipeDef ParentName="SurgeryFlesh">
		<defName>RS_InstallWoodenTail</defName>
		<label>Install wooden tail</label>
		<description>가짜 나무 하단꼬리를 이식합니다.</description>
		<jobString>Installing wooden tail.</jobString>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<workAmount>2500</workAmount>
		<skillRequirements>
		  <Medicine>3</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
			  <filter>
				  <categories>
					  <li>Medicine</li>
					</categories>
				</filter>
			  <count>2</count>
			</li>
		  	<li>
				<filter>
				  <thingDefs>
					<li>WoodLog</li>
				  </thingDefs>
				</filter>
				<count>1</count>
			  </li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
		 	<thingDefs>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
		<descriptionHyperlinks>
			<ThingDef>WoodLog</ThingDef>
			<HediffDef>RS_WoodenTail</HediffDef>
		</descriptionHyperlinks>
		<appliedOnFixedBodyParts>
			<li>RS_LowerTail</li>
		</appliedOnFixedBodyParts>
		<addsHediff>RS_WoodenTail</addsHediff>
	</RecipeDef>

