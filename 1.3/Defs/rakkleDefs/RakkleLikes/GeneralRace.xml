        <minAgeForAdulthood>16</minAgeForAdulthood>
        <maleGenderProbability>0.2</maleGenderProbability>
        <!--<useOnlyPawnkindBackstories>true</useOnlyPawnkindBackstories>-->
        <immuneToAge>false</immuneToAge>
        <humanRecipeImport>true</humanRecipeImport>
        <canLayDown>true</canLayDown>
        <maxDamageForSocialfight>6</maxDamageForSocialfight>
        <immuneToXenophobia>true</immuneToXenophobia>
        <disallowedTraits>
          <li>
            <defName>Beauty</defName>
            <degree>-2</degree>
          </li>
          <li>
            <defName>Beauty</defName>
            <degree>-1</degree>
          </li>
          <li>
            <defName>AnnoyingVoice</defName>
          </li>
          <li>
            <defName>CreepyBreathing</defName>
          </li>
          <li>
            <defName>Xenophobia</defName>
          </li>
          <li>
            <defName>BodyPurist</defName>
          </li>
          <li>
            <defName>Ascetic</defName>
          </li>
        </disallowedTraits>

        <chemicalSettings>
          <li>
            <chemical>RakkleShedPromoter</chemical>
            <reactions>
              <li Class="IngestionOutcomeDoer_GiveHediff">
                <hediffDef>Hediff_RakkleShedPromoterM</hediffDef>
                <chance>0.3</chance>
                <doToGeneratedPawnIfAddicted>false</doToGeneratedPawnIfAddicted>
